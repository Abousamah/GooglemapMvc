@{
    ViewBag.Title = "Image Map Type";
    Layout = "~/Views/Shared/_LayoutExamples.cshtml";
}

@(Html.GoogleMap()
        .Name("map")
        .Zoom(1)
        .Height(520)
        .Latitude(0)
        .Longitude(0)
        .ZoomControlStyle(ZoomControlStyle.Large)
        .StreetViewControlVisible(false)
        
        .ImageMapTypes(map =>
        {
            map.Add()
                .TileUrlPattern("~/maptype/GetLabeledTile?x={0}&y={1}&zoom={2}&tileWith={3}&tileHeigth={4}")
                .TileSize(new Size(256, 256))
                .RepeatHorizontally(false)
                .RepeatVertically(false)
                .MaxZoom(4)
                .MinZoom(1)
                .MapTypeName("Labels")
                .MapTypeAltName("Game of Thrones Labeled");
            

            map.Add()
                .TileUrlPattern("~/maptype/GetNaturalTile?x={0}&y={1}&zoom={2}&tileWith={3}&tileHeigth={4}")
                .TileSize(new Size(256, 256))
                .RepeatHorizontally(false)
                .RepeatVertically(false)
                .MaxZoom(4)
                .MinZoom(1)
                .MapTypeName("No Labels")
                .MapTypeAltName("Game of Thrones Natural Map");

        })
        .MapTypeId("Labels")

)